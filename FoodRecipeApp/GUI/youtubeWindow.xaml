<Window x:Class="FoodRecipeApp.GUI.youtubeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FoodRecipeApp.GUI"
        WindowStartupLocation="CenterScreen"
        Loaded="ChildWindow_Loaded"
        mc:Ignorable="d"
        Background="AliceBlue"
        Icon="/Resources/recipe-book.ico"
        Title="youtubeWindow" Height="530" Width="931.959">
    <Canvas>
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="2*"/>
                <RowDefinition Height="8*"/>
            </Grid.RowDefinitions>
        
        <Label Content="Video hướng dẫn" Grid.Row="0"
               FontFamily="Segoe UI Light" FontSize="20" Margin="10"/>
        <DockPanel Margin="10" Grid.Row="1">
            <WebBrowser
				x:Name="DishMedia"
                VerticalAlignment="Center"
                HorizontalAlignment="Center"
				Width="900"
				Height="420" />
        </DockPanel>
        </Grid>
    </Canvas>
</Window>
