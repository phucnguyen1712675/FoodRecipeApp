	<Page.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="/FoodRecipeApp;component/RecipeResources.xaml" />
			</ResourceDictionary.MergedDictionaries>
			
			<viewModels:RecipeViewModel x:Key = "MyRecipeViewModel" />
			<converters:TileConverter x:Key="MyTileConverter"/>
		</ResourceDictionary>
		
		
		<!--<Style TargetType="telerik:Tile">
			<Setter Property="TileType" Value="{Binding Content.Loai, Converter={StaticResource MyTileConverter}, RelativeSource={RelativeSource Self}}" />
		</Style>-->
	</Page.Resources>
	<Grid	x:Name="LayoutRoot"
			HorizontalAlignment="Stretch"
			VerticalAlignment="Stretch"
			DataContext="{StaticResource MyRecipeViewModel}">
		<telerik:RadTileList x:Name="RadTileList" 
							 ItemsSource="{Binding Recipes}"
							 ScrollViewer.HorizontalScrollBarVisibility="Hidden"
							 AutoGeneratingTile="OnAutoGeneratingTile"
                             GroupMember="Title" 
                             GroupTemplate="{DynamicResource GroupTemplate}"
                             SelectionMouseButton="{Binding SelectedValue, ElementName=selectMouseButtonComboBox, Mode=TwoWay}"
                             GroupHeaderHeight="{Binding Text, ElementName=groupHeaderHeightTextBox, Mode=TwoWay}"
                             GroupHeaderVisibility="{Binding SelectedValue, ElementName=groupHeaderVisibilityComboBox, Mode=TwoWay}"
                             TileReorderMode="{Binding SelectedValue, ElementName=tileReorderModeComboBox, Mode=TwoWay}"
                             VerticalTilesAlignment="{Binding SelectedValue, ElementName=verticalTilesAlignmentComboBox, Mode=TwoWay}" 
                             ItemTemplate="{DynamicResource TileListItemTemplate}">
			<telerik:RadTileList.PanoramaBackground>
				<Image Source="/Resources/panoramic-view-sea.jpg" 
					   Stretch="UniformToFill"/>
			</telerik:RadTileList.PanoramaBackground>
		</telerik:RadTileList>
	</Grid>
	

